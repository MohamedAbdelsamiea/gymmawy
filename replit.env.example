# Replit Environment Variables
# Copy this to .env in your Replit project

# Database Configuration (Replit will provide this)
DATABASE_URL=postgresql://postgres:password@localhost:5432/gymmawy

# Server Configuration
NODE_ENV=development
PORT=3000

# JWT Configuration
JWT_SECRET=gymmawy-super-secret-jwt-key-2024-replit
JWT_EXPIRES_IN=7d
JWT_ISSUER=gymmawy

# CORS Configuration
CORS_ORIGIN=https://your-repl-url.replit.dev

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security
BCRYPT_ROUNDS=12
SESSION_SECRET=gymmawy-session-secret-2024

# Frontend URL (will be your Replit URL)
FRONTEND_URL=https://your-repl-url.replit.dev
