{
  "name": "gymmawy",
  "version": "1.0.0",
  "description": "Gymmawy PERN Stack Fitness Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cd gymmawy-backend && npm run dev\" \"cd gymmawy-frontend && npm run dev\"",
    "start": "cd gymmawy-backend && npm start",
    "build": "cd gymmawy-frontend && npm run build",
    "install:all": "cd gymmawy-backend && npm install && cd ../gymmawy-frontend && npm install",
    "export-data": "cd gymmawy-backend && npm run export-data",
    "import-data": "cd gymmawy-backend && npm run import-data",
    "setup": "npm run install:all && cd gymmawy-backend && npx prisma generate && npx prisma db push"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
    "gymmawy-backend",
    "gymmawy-frontend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "fitness",
    "gym",
    "pern",
    "postgresql",
    "express",
    "react",
    "nodejs"
  ],
  "author": "Gymmawy Team",
  "license": "MIT"
}
